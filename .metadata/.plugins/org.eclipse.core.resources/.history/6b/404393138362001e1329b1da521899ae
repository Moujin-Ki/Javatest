package ex06;

import java.util.function.IntFunction;
import java.util.function.ToIntFunction;

public class Lambda9_1 {
	public static int main(String[] args) {
		IntFunction<Integer[]> exam = 
				(int[] arr) -> {
				int sum = 0;
				for(int i : arr)
					sum += i;
				return sum;
				};
		
				
		int[] arr = {1,2,3,4,5};
			
		int result = int[] arr;
		System.out.println(result);
	}
}
